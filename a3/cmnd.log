0) cat /etc/passwd

1)
sed -n '/^1\.0/ p' 2019-10.csv > vendor2.csv
wc vendor2.csv

2)
sed 's|[:"-]|| g' 2019-10.csv > no-separators.csv
head -10 no-separators.csv

3)
sed -E 's/([0-9]+)\.[0-9]+/\1/g' no-separators.csv > no-fractions.csv
head -10 no-fractions.csv

4) awk 'BEGIN { FS = "," } $17 >= 10.0 { tips[$14]++ } END { for (tip in tips) { print tip, tips[tip] } }' 2019-10.csv > results.txt

5) awk 'BEGIN { OFS="\t" } { print $2, $1 }' results.txt | sort -rn | head -n 5 | awk '{ print $2, $1 }'
